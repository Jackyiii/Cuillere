{
  "name": "exposition",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "move-dll": "cross-env cp vendor/vendors.dll.js bundle",
    "build-dll": "webpack -p --config webpack.dll.config.js && npm run move-dll",
    "build-fast": "cross-env PROD_ENV=1 webpack -p --config webpack.prod.config.js && npm run move-dll",
    "build": "npm run build-dll && npm run build-fast",
    "profile": "webpack --config webpack.prod.config.js --profile --json > stats.json",
    "dev-build": "webpack --config webpack.dev.config.js",
    "dev": "webpack-dev-server --config webpack.dev.config.js --open"
  },
